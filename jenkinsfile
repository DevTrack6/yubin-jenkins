pipeline {
    agent any
    stages {
        stage('hello') {
            parallel {
                stage("hello1") {
                    steps {
                        sh 'echo "hello1"'
                    }
                }
                stage("hello2") {
                    steps {
                        sh 'echo "hello2"'
                    }
                }
            }
            steps {
                sh 'echo "hello"'
            }
        }
        stage("checkout") {
            steps {
                sh 'echo "checkout"'
                checkout scm
                sh 'ls -al'
            }
        }
        stage("test") {
            steps {
                sh 'echo "test"'
                sh 'ls -al'
            }
        }
    }
}
